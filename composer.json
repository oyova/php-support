{
    "name": "oyova/php-support",
    "description": "General support functionality for Oyova PHP projects.",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Oyova\\PhpSupport\\": "src/"
        }
    },
    "require": {
        "php": ">=7.4,<8.3",
        "ryanwhitman/php-values": "^1.4"
    },
    "require-dev": {
        "illuminate/collections": "^8.83",
        "oyova/php-cs-fixer": "^1.4",
        "pestphp/pest": "^1.22",
        "symfony/var-dumper": "^5.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
